; test.mmp - Basic functionality test for the MMP-1

&TestDisplays Step
@ RetTestDisplays

&TestALU Step
@ RetTestALU

; 0 ClockMode

&TestDisplays DispA



; Test 7 seg displays

@ Const_As = 0xAAAAAAAA
@ Const_Bs = 0xBBBBBBBB

@ TestDisplays
    Const_As DispA
    Const_Bs DispB

&RetTestDisplays Step



; Test ALU

@ Const_2 = 2
@ Const_3 = 3

@ TestALU
    Const_2 ALU.A
    Const_3 ALU.B
    ALU.SUM DispA

&RetTestALU Step
